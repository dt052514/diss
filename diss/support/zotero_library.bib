  author = {Al-Fattah, Saud M.},
  file = {/Users/davidtingle/Dropbox/Academic/Zotero/Zotero Files//2013/Al-Fattah_2013_(National Oil Companies).pdf}
}

@article{hopf_common-sense_2013,
