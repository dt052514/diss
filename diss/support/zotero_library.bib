@article{humphreys_applying_2013-1,
  langid = {english},
  title = {Applying {{Jackson}}'s {{Methodological Ideal}}-{{Types}}: {{Problems}} of {{Differentiation}} and {{Classification}}},
  volume = {41},
  url = {http://mil.sagepub.com/content/41/2/290},
  shorttitle = {Applying {{Jackson}}'s {{Methodological Ideal}}-{{Types}}},
  abstract = {In The Conduct of Inquiry in International Relations, Patrick Jackson situates methodologies in International Relations in relation to their underlying philosophical assumptions. One of his aims is to map International Relations debates in a way that `capture[s] current controversies' (p. 40). This ambition is overstated: whilst Jackson's typology is useful as a clarificatory tool, (re)classifying existing scholarship in International Relations is more problematic. One problem with Jackson's approach is that he tends to run together the philosophical assumptions which decisively differentiate his methodologies (by stipulating a distinctive warrant for knowledge claims) and the explanatory strategies that are employed to generate such knowledge claims, suggesting that the latter are entailed by the former. In fact, the explanatory strategies which Jackson associates with each methodology reflect conventional practice in International Relations just as much as they reflect philosophical assumptions. This makes it more difficult to identify each methodology at work than Jackson implies. I illustrate this point through a critical analysis of Jackson's controversial reclassification of Waltz as an analyticist, showing that whilst Jackson's typology helps to expose inconsistencies in Waltz's approach, it does not fully support the proposed reclassification. The conventional aspect of methodologies in International Relations also raises questions about the limits of Jackson's `engaged pluralism'.},
  number = {2},
  journaltitle = {Millennium - Journal of International Studies},
  urldate = {2013-03-04},
  date = {2013},
  pages = {290--308},
  keywords = {Methodology,explanation,international relations theory},
  author = {Humphreys, Adam R. C.}
}

